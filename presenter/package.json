{
  "name": "dreammall-presenter",
  "version": "1.2.1",
  "description": "The DreamMall presenter",
  "main": "build/index.cjs",
  "type": "module",
  "keywords": [
    "nodejs",
    "npm",
    "docker",
    "jq",
    "vue",
    "vike",
    "vuetify",
    "pinia",
    "vue-i18n",
    "eslint",
    "stylelint",
    "vitest",
    "storybook"
  ],
  "author": {
    "name": "Ulf Gebhardt"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dreammall-earth/dreammall.earth/issues"
  },
  "homepage": "https://github.com/dreammall-earth/dreammall.earth/blob/master/presenter/README.md",
  "scripts": {
    "dev": "npm run server:dev",
    "prod": "npm run build && npm run server:prod",
    "build": "vite build && npm run server:build",
    "server": "tsx ./server/index.ts",
    "server:dev": "npm run server",
    "server:prod": "cross-env NODE_ENV=production node ./build/index.cjs",
    "server:prod:ts": "cross-env NODE_ENV=production npm run server",
    "server:build": "tsx scripts/buildServer/buildServer",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o build/storybook",
    "storybook:test": "test-storybook",
    "test:lint": "npm run test:lint:eslint && npm run test:lint:style && npm run test:lint:locales",
    "test:lint:eslint": "eslint --ext .vue,.ts,.tsx,.js,.jsx,.cjs,.mjs,.json,.yml,.yaml --max-warnings 0 .",
    "test:lint:locales": "scripts/locales/locales.sh src/locales",
    "test:lint:style": "stylelint --max-warnings 0 --ignore-path .gitignore \"**/*.{css,scss,vue,vuex}\"",
    "test:unit": "npm run test:unit:dev -- run --coverage",
    "test:unit:update": "npm run test:unit:dev -- run --coverage -u",
    "test:unit:dev": "vitest",
    "test": "npm run test:lint && npm run test:unit",
    "update": "npx npm-check-updates"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@mdi/font": "^7.4.47",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/apollo-composable": "^4.2.1",
    "@vue/apollo-option": "^4.2.0",
    "@vue/compiler-sfc": "^3.5.5",
    "@vue/server-renderer": "3.4.38",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "pinia": "^2.2.2",
    "pinia-plugin-persistedstate": "^4.0.1",
    "sirv": "^2.0.4",
    "validator": "^13.12.0",
    "vike": "^0.4.195",
    "vite": "^5.4.5",
    "vue": "3.4.38",
    "vue-i18n": "^9.13.1",
    "vuetify": "^3.5.15"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@intlify/eslint-plugin-vue-i18n": "^3.0.0",
    "@pinia/testing": "^0.1.5",
    "@storybook/addon-essentials": "^8.3.0",
    "@storybook/addon-interactions": "^8.3.0",
    "@storybook/addon-links": "^8.3.0",
    "@storybook/blocks": "^8.3.0",
    "@storybook/test-runner": "^0.19.1",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^8.3.1",
    "@storybook/vue3-vite": "^8.3.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.5.5",
    "@types/validator": "^13.12.1",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^2.1.1",
    "@vue/test-utils": "^2.4.6",
    "esbuild": "^0.23.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-local-rules": "^3.0.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vitest": "^0.4.1",
    "eslint-plugin-vue": "^9.28.0",
    "eslint-plugin-vuetify": "^2.4.0",
    "eslint-plugin-yml": "^1.14.0",
    "fs-extra": "^11.2.0",
    "happy-dom": "^15.7.4",
    "mock-apollo-client": "^1.3.1",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "1.78.0",
    "sass-loader": "^16.0.1",
    "storybook": "^8.1.9",
    "stylelint": "^16.9.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recess-order": "^5.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "tsx": "^4.19.1",
    "typescript": "^5.4.5",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "^2.0.4",
    "vue-tsc": "^1.8.27"
  },
  "imports": {
    "#components/*": "./src/components/*",
    "#pages/*": "./src/pages/*",
    "#assets/*": "./src/assets/*",
    "#layouts/*": "./src/layouts/*",
    "#stores/*": "./src/stores/*",
    "#mutations/*": "./src/graphql/mutations/*",
    "#queries/*": "./src/graphql/queries/*",
    "#src/*": "./src/*",
    "#plugins/*": "./renderer/plugins/*",
    "#context/*": "./renderer/context/*",
    "#tests/*": "./scripts/tests/*",
    "#types/*": "./types/*",
    "#root/*": "./*"
  }
}
