{
  "name": "dreammall-frontend",
  "version": "1.0.0",
  "description": "The DreamMall frontend",
  "main": "build/index.js",
  "type": "module",
  "keywords": [
    "nodejs",
    "npm",
    "docker",
    "jq",
    "vue",
    "vike",
    "vuetify",
    "pinia",
    "vue-i18n",
    "eslint",
    "stylelint",
    "vitest",
    "storybook"
  ],
  "author": {
    "name": "Ulf Gebhardt"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dreammall-earth/dreammall.earth/issues"
  },
  "homepage": "https://github.com/dreammall-earth/dreammall.earth/blob/master/frontend/README.md",
  "scripts": {
    "dev": "npm run server:dev",
    "prod": "npm run build && npm run server:prod",
    "build": "vite build  && npm run server:build",
    "server": "node --loader ts-node/esm ./server/index.ts",
    "server:dev": "npm run server",
    "server:prod": "cross-env NODE_ENV=production node ./build/index.cjs",
    "server:prod:ts": "cross-env NODE_ENV=production npm run server",
    "server:build": "tsx scripts/buildServer/buildServer",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o build/storybook",
    "storybook:test": "test-storybook",
    "test:lint": "npm run test:lint:eslint && npm run test:lint:style && npm run test:lint:locales",
    "test:lintfix": "npm run test:lint:eslint -- --fix && npm run test:lint:style -- --fix && npm run test:lint:locales -- --fix",
    "test:lint:eslint": "eslint --ext .vue,.ts,.tsx,.js,.jsx,.cjs,.mjs,.json,.yml,.yaml --max-warnings 0 .",
    "test:lint:locales": "scripts/locales/locales.sh src/locales",
    "test:lint:style": "stylelint --max-warnings 0 --ignore-path .gitignore \"**/*.{css,scss,vue,vuex}\"",
    "test:unit": "npm run test:unit:dev -- run --coverage",
    "test:unit:update": "npm run test:unit:dev -- run --coverage -u",
    "test:unit:dev": "vitest",
    "test": "npm run test:lint && npm run test:unit",
    "update": "npx npm-check-updates"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@chenfengyuan/vue-qrcode": "^2.0.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@mdi/font": "^7.4.47",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/apollo-composable": "^4.2.1",
    "@vue/apollo-option": "^4.0.0",
    "@vue/apollo-ssr": "^4.0.0",
    "@vue/compiler-sfc": "^3.5.4",
    "@vue/server-renderer": "3.4.35",
    "compression": "^1.7.4",
    "cookie": "^0.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.16.0",
    "iban": "^0.0.14",
    "js-cookie": "^3.0.5",
    "oidc-client-ts": "^3.0.1",
    "pinia": "^2.2.2",
    "pinia-plugin-persistedstate": "^3.2.3",
    "qrcode": "^1.5.4",
    "sirv": "^2.0.4",
    "three": "^0.168.0",
    "vike": "^0.4.194",
    "vite": "^5.4.5",
    "vite-svg-loader": "^5.1.0",
    "vue": "3.4.35",
    "vue-i18n": "9.13.0",
    "vue3-toastify": "^0.2.3",
    "vuetify": "^3.7.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.3.0",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@pinia/testing": "^0.1.5",
    "@storybook/addon-essentials": "^8.3.0",
    "@storybook/addon-interactions": "^8.3.0",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/blocks": "^8.3.0",
    "@storybook/test": "^8.2.9",
    "@storybook/test-runner": "^0.19.1",
    "@storybook/vue3": "^8.2.9",
    "@storybook/vue3-vite": "^8.3.0",
    "@types/compression": "^1.7.5",
    "@types/cookie": "^0.6.0",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/iban": "^0.0.35",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.14.8",
    "@types/three": "^0.168.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.5.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vue/test-utils": "^2.4.6",
    "esbuild": "^0.23.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vitest": "^0.4.1",
    "eslint-plugin-vue": "^9.28.0",
    "eslint-plugin-vuetify": "^2.4.0",
    "eslint-plugin-yml": "^1.14.0",
    "fs-extra": "^11.2.0",
    "happy-dom": "^14.12.3",
    "mock-apollo-client": "^1.3.1",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "1.78.0",
    "sass-loader": "^16.0.0",
    "storybook": "^8.3.0",
    "stylelint": "^16.9.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.15.7",
    "typescript": "^5.6.2",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^2.0.4",
    "vue-component-type-helpers": "^2.1.6",
    "vue-tsc": "^2.0.29"
  },
  "imports": {
    "#components/*": "./src/components/*",
    "#pages/*": "./src/pages/*",
    "#assets/*": "./src/assets/*",
    "#layouts/*": "./src/layouts/*",
    "#queries/*": "./src/graphql/queries/*",
    "#mutations/*": "./src/graphql/mutations/*",
    "#subscriptions/*": "./src/graphql/subscriptions/*",
    "#stores/*": "./src/stores/*",
    "#src/*": "./src/*",
    "#plugins/*": "./renderer/plugins/*",
    "#context/*": "./renderer/context/*",
    "#tests/*": "./scripts/tests/*",
    "#types/*": "./types/*",
    "#root/*": "./*"
  }
}
